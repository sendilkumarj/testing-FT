  	/* === styles added for all the Jquery tabs across FT ===== Added by sendil 08/04/2014 */ 
  .ui-state-active a:link {background:#f2f2f2 !important; background-color: #f2f2f2 !important; border-bottom: 1px solid #fff !important;}
  .ui-tabs-nav > .ui-state-default a{background-color: #f6f6f6 !important; background: #f6f6f6 !important;top: 1px !important;}
  .ui-tabs-nav > .ui-state-active a{border-color:#fff !important; background:#fff !important; background-color: #fff !important; color:#333 !important; }
  
  .ui-tabs .ui-tabs-nav li{border: 1px solid #ccc !important;/* border-bottom:0px !important;*/  }
  .ui-tabs-nav .ui-state-default{ margin: 0px -1px 0px 0px !important; }
  
  .ui-tabs-nav > .ui-state-hover a{background-color: #f0f0f0 !important; background: #f0f0f0 !important;}
  .ui-state-active a:hover{background:#fff !important; background-color: #fff !important; border-bottom: 1px solid #fff !important;}
  
