        function dragging(event){     
            var node = event.target.getAttribute('data-scrollable');   
            var scrPos = $(event.target).position().top;  
            //alert($(event.target).attr('data-scrollid')); 
            $('.'+node).css({top: -(scrPos*(xparHArray[ $(event.target).attr('data-scrollid') ])-1)});
