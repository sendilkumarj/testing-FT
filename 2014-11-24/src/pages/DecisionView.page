            //$('#durTotal').css('display','none');
            var data1 = gridData;
            
            // To enable kendo sorting.
            function SortByValue1(x,y) {
                return x.Position- y.Position; 
            }
            
            if(data1 != null){
                data1.sort(SortByValue1);
            }
            
            // To remove the cache on kendo table
            $.ajaxSetup({ cache: false }); 
            
